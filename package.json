  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "dotenv": "^7.0.0",
    "express": "^4.18.2",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.9",
    "mongoose-mongodb-errors": "0.0.2",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.19.4",
    "validator": "^10.11.0"
  }
}
